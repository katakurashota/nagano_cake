<div class ="container">
  <%= form_with model:@customer, url:'/customer/:id', local:true do |f| %>
 <div class ="row">
   <h5>会員情報編集</h5>
 </div>
 <div class ="row">
   <div class="col-md-2">
     <p>氏名</p>
   </div>
   <div class ="col-md-8">
   <%= f.text_field :last_name %>
   <%= f.text_field :first_name %>
   </div>
 </div>
 <% end %>
</div>